periodogram name rate n=100:
    python pdgram_lomb_scargle.py --folder ../data/catalog2023/ --name {{name}} --n {{n}} --rate {{rate}} --outdir ../output/img/

    python greedy_harmonic_sum.py --pgram ../data/catalog2023/{{name}}/pdgram-LombScargle.npy --grid ../data/catalog2023/{{name}}/pdgram-freq-grid.npy --method "Lomb-Scargle" --outdir ../output/img/{{name}}/
    
    python pdgram_pdm.py --folder ../data/catalog2023/ --name {{name}} --n {{n}} --rate {{rate}} --outdir ../output/img/
    
    python greedy_harmonic_sum.py --pgram ../data/catalog2023/{{name}}/pdgram-PhaseDispMin.npy --grid ../data/catalog2023/{{name}}/pdgram-PhaseDispMin-freq-grid.npy --outdir ../output/img/{{name}}/ --method PDM --inverse true
    
    python pdgram_duty_cycle.py --folder ../data/catalog2023/ --name {{name}} --n {{n}} --rate {{rate}} --outdir ../output/img/
    
    python greedy_harmonic_sum.py --pgram ../data/catalog2023/{{name}}/pdgram-DutyCycle.npy --grid ../data/catalog2023/{{name}}/pdgram-freq-grid.npy --outdir ../output/img/{{name}}/ --method "Duty Cycle"

